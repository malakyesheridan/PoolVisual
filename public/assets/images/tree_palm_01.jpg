<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="600" viewBox="0 0 400 600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="600" fill="#f8f9fa"/>
  
  <!-- Palm Tree Trunk -->
  <rect x="180" y="200" width="40" height="400" fill="#8B4513" rx="20"/>
  
  <!-- Palm Tree Leaves -->
  <g fill="#228B22">
    <!-- Left leaves -->
    <ellipse cx="120" cy="180" rx="60" ry="20" transform="rotate(-30 120 180)"/>
    <ellipse cx="100" cy="160" rx="50" ry="15" transform="rotate(-45 100 160)"/>
    <ellipse cx="90" cy="140" rx="40" ry="12" transform="rotate(-60 90 140)"/>
    
    <!-- Right leaves -->
    <ellipse cx="280" cy="180" rx="60" ry="20" transform="rotate(30 280 180)"/>
    <ellipse cx="300" cy="160" rx="50" ry="15" transform="rotate(45 300 160)"/>
    <ellipse cx="310" cy="140" rx="40" ry="12" transform="rotate(60 310 140)"/>
    
    <!-- Center leaves -->
    <ellipse cx="200" cy="150" rx="40" ry="15" transform="rotate(0 200 150)"/>
    <ellipse cx="200" cy="130" rx="35" ry="12" transform="rotate(0 200 130)"/>
  </g>
  
  <!-- Ground shadow -->
  <ellipse cx="200" cy="580" rx="80" ry="20" fill="#000000" opacity="0.2"/>
</svg>
