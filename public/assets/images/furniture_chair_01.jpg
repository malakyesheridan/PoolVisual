<?xml version="1.0" encoding="UTF-8"?>
<svg width="200" height="300" viewBox="0 0 200 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="200" height="300" fill="#f8f9fa"/>
  
  <!-- Pool Chair Frame -->
  <rect x="20" y="50" width="160" height="20" fill="#C0C0C0" rx="10"/>
  <rect x="20" y="200" width="160" height="20" fill="#C0C0C0" rx="10"/>
  
  <!-- Chair Back -->
  <rect x="30" y="50" width="140" height="150" fill="#FFFFFF" stroke="#C0C0C0" stroke-width="2" rx="5"/>
  
  <!-- Chair Seat -->
  <rect x="30" y="200" width="140" height="80" fill="#FFFFFF" stroke="#C0C0C0" stroke-width="2" rx="5"/>
  
  <!-- Chair Legs -->
  <rect x="40" y="280" width="8" height="20" fill="#C0C0C0"/>
  <rect x="152" y="280" width="8" height="20" fill="#C0C0C0"/>
  
  <!-- Cushion -->
  <rect x="40" y="210" width="120" height="60" fill="#E6F3FF" stroke="#B3D9FF" stroke-width="1" rx="3"/>
  
  <!-- Ground shadow -->
  <ellipse cx="100" cy="295" rx="60" ry="5" fill="#000000" opacity="0.2"/>
</svg>
