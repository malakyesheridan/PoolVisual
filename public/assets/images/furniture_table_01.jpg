<?xml version="1.0" encoding="UTF-8"?>
<svg width="300" height="200" viewBox="0 0 300 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="200" fill="#f8f9fa"/>
  
  <!-- Pool Table Top -->
  <rect x="20" y="20" width="260" height="160" fill="#FFFFFF" stroke="#C0C0C0" stroke-width="2" rx="10"/>
  
  <!-- Table Legs -->
  <rect x="40" y="180" width="12" height="20" fill="#C0C0C0"/>
  <rect x="248" y="180" width="12" height="20" fill="#C0C0C0"/>
  
  <!-- Table Surface Pattern -->
  <rect x="30" y="30" width="240" height="140" fill="#F0F8FF" stroke="#B3D9FF" stroke-width="1" rx="5"/>
  
  <!-- Umbrella Hole -->
  <circle cx="150" cy="100" r="15" fill="#E6F3FF" stroke="#B3D9FF" stroke-width="1"/>
  
  <!-- Ground shadow -->
  <ellipse cx="150" cy="195" rx="80" ry="5" fill="#000000" opacity="0.2"/>
</svg>
