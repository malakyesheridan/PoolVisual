<?xml version="1.0" encoding="UTF-8"?>
<svg width="100" height="150" viewBox="0 0 100 150" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="100" height="150" fill="#f8f9fa"/>
  
  <!-- Palm Tree Trunk -->
  <rect x="40" y="50" width="20" height="100" fill="#8B4513" rx="10"/>
  
  <!-- Palm Tree Leaves -->
  <g fill="#228B22">
    <!-- Left leaves -->
    <ellipse cx="25" cy="40" rx="25" ry="8" transform="rotate(-30 25 40)"/>
    <ellipse cx="20" cy="35" rx="20" ry="6" transform="rotate(-45 20 35)"/>
    
    <!-- Right leaves -->
    <ellipse cx="75" cy="40" rx="25" ry="8" transform="rotate(30 75 40)"/>
    <ellipse cx="80" cy="35" rx="20" ry="6" transform="rotate(45 80 35)"/>
    
    <!-- Center leaves -->
    <ellipse cx="50" cy="30" rx="15" ry="6" transform="rotate(0 50 30)"/>
  </g>
  
  <!-- Ground shadow -->
  <ellipse cx="50" cy="145" rx="30" ry="5" fill="#000000" opacity="0.2"/>
</svg>
